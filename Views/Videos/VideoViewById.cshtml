@model CastafraySoundCatalog.Models.VideoModel

@{
    ViewBag.Title = "VideoViewById";
}

<div>
    <hr />
    @Html.ActionLink("Back to List", "VideoManager")
    <hr />
</div>

<div class="form-group">
    <label for="@Model.VideoName"></label>
    <div class="col-md-10">
        <video autoplay controls src="~/video/@Url.Content(Model.Title)"></video>
    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
